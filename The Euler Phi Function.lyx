\begin_layout Lemma
\begin_inset CommandInset label
LatexCommand label
name "Lemma-[5]-generates-half -of-U_2^n"

\end_inset

For 
\begin_inset Formula $n\ge1$
\end_inset


\begin_inset Formula 
\[
5^{2^{n}}\equiv1\left(\mod\,2^{n+2}\right),\quad5^{2^{n}}\not\equiv1\left(\mod\,2^{n+3}\right).
\]

\end_inset


\end_layout

\begin_layout Proof
For 
\begin_inset Formula $n=1$
\end_inset


\begin_inset Formula 
\[
5^{2}=1+24\equiv1\left(\mod\,8\right),\quad5^{2}\not\equiv1\left(\mod\,16\right).
\]

\end_inset

Assume that the result is true for 
\begin_inset Formula $n$
\end_inset

.
 Then
\begin_inset Formula 
\[
5^{2^{n}}=1+m2^{n+2},\quad2\nmid m.
\]

\end_inset

Now
\begin_inset Formula 
\begin{eqnarray*}
5^{2^{n+1}} & = & \left(5^{2^{n}}\right)^{2}\\
 & = & 1+m2^{n+3}+m^{2}2^{2\left(n+3\right)}\\
 & = & 1+\left(m+m^{2}2^{n+3}\right)2^{n+3}
\end{eqnarray*}

\end_inset

so
\begin_inset Formula 
\[
5^{2^{n+1}}\equiv1\left(\mod\,2^{n+3}\right),\quad5^{2^{n+1}}\not\equiv1\left(\mod\,2^{n+4}\right)
\]

\end_inset

the last non-congruence is due to the fact that 
\begin_inset Formula $2\nmid m$
\end_inset

.
 
\end_layout

