\mathbf{U}_{13} & = & \left\{ \left[1\right]_{13},\left[2\right]_{13},\left[3\right]_{13},\left[4\right]_{13},\left[5\right]_{13},\left[6\right]_{13},\left[7\right]_{13},\left[8\right]_{13},\left[9\right]_{13},\left[10\right]_{13},\left[11\right]_{13},\left[12\right]_{13}\right\} \\
 & = & \left\langle 2\right\rangle \simeq\mathbf{Z}/10\mathbf{Z}\\
\mathbf{U}_{16} & = & \left\{ \left[1\right]_{16},\left[3\right]_{16},\left[5\right]_{16},\left[7\right]_{16},\left[9\right]_{16},\left[11\right]_{16},\left[13\right]_{16},\left[15\right]_{16}\right\} \\
 & = & \left\{ \left[1\right]_{16},\left[5\right]_{16},\left[9\right]_{16},\left[13\right]_{16}\right\} \bigcup\left\{ \left[3\right]_{16},\left[7\right]_{16},\left[11\right]_{16},\left[15\right]_{16}\right\} \\
 & = & \left\{ \left[1\right]_{16},\left[5\right]_{16},\left[5\right]_{16}^{2},\left[5\right]_{16}^{3}\right\} \bigcup\left\{ \left[7\right]_{16}\cdot\left[5\right]_{16},\left[7\right]_{16}\cdot\left[1\right]_{16},\left[7\right]_{16}\cdot\left[5\right]_{16}^{3},\left[7\right]_{16}\cdot\left[5\right]_{16}^{2}\right\} \\
 & \simeq & \left\{ \left[1\right]_{16},\left[7\right]_{16},\times\right\} \times\left\{ \left[1\right]_{16},\left[5\right]_{16},\left[5\right]_{16}^{2},\left[5\right]_{16}^{3}\right\} \\
 & \simeq & \left(\mathbf{Z}/2\mathbf{Z}\right)\times\left(\mathbf{Z}/4\mathbf{Z}\right).
\end{eqnarray*}

\end_inset


