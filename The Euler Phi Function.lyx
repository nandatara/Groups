 & = & \left\{ \left[0\right]_{6},\left[1\right]_{6},\left[2\right]_{6},\left[3\right]_{6},\left[4\right]_{6},\left[5\right]_{6},+\right\} \\
\mathbf{U}_{8} & = & \left\{ \left[1\right]_{8},\left[3\right]_{8},\left[5\right]_{8},\left[7\right]_{8}\right\} \simeq\mathbf{Z}/2\mathbf{Z}\times\mathbf{Z}/2\mathbf{Z}\\
 & = & \left\{ \left(\left[0\right]_{2},\left[0\right]_{2}\right),\left(\left[0\right]_{2},\left[1\right]_{2}\right),\left(\left[1\right]_{2},\left[0\right]_{2}\right),\left(\left[1\right]_{2},\left[1\right]_{2}\right),+\right\} \\
\mathbf{U}_{3^{2}} & = & \left\{ \left[1\right]_{9},\left[2\right]_{9},\left[4\right]_{9},\left[5\right]_{9},\left[7\right]_{9},\left[8\right]_{9}\right\} \\
